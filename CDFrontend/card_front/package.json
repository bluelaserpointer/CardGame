{
  "name": "card_front",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "babel src -d lib",
    "test:unit": "vue-cli-service test:unit",
    "lint": "eslint --ext .js,.vue src",
    "build:prod": "vue-cli-service build",
    "build:stage": "vue-cli-service build --mode staging",
    "dev": "vue-cli-service serve",
    "new": "plop",
    "preview": "node build/index.js --preview",
    "svgo": "svgo -f src/icons/svg --config=src/icons/svgo.yml",
    "test:all": "vue-cli-service test:unit --coverage --watch",
    "test:ci": "npm run lint && npm run test:unit",
    "test:cov": "vue-cli-service test:unit --coverage",
    "test:watch": "vue-cli-service test:unit --watch",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "webpack-dev-server",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "devO": "start-test 8080 cy:open",
    "report:coverage": "nyc report",
    "coveralls": "nyc report --reporter=text-lcov | coveralls"
  },
  "dependencies": {
    "@foreachbe/cypress-tinymce": "^1.0.0",
    "axios": "^0.18.1",
    "babel-plugin-require-context-hook": "^1.0.0",
    "babel-plugin-transform-require-context": "^0.1.1",
    "babel-preset-env": "^1.7.0",
    "clipboard": "2.0.4",
    "codemirror": "5.45.0",
    "core-js": "3.6.5",
    "driver.js": "0.9.5",
    "dropzone": "5.5.1",
    "echarts": "4.2.1",
    "element-ui": "2.13.2",
    "file-saver": "2.0.1",
    "flush-promises": "^1.0.2",
    "fuse.js": "3.4.4",
    "jest-config": "latest",
    "js-cookie": "2.2.0",
    "jsonlint": "1.6.3",
    "jszip": "3.2.1",
    "moment": "^2.27.0",
    "normalize.css": "7.0.0",
    "nprogress": "0.2.0",
    "path-to-regexp": "2.4.0",
    "screenfull": "4.2.0",
    "script-loader": "0.7.2",
    "sortablejs": "1.8.4",
    "tui-editor": "1.3.3",
    "vue": "2.6.10",
    "vue-count-to": "1.0.13",
    "vue-router": "3.0.2",
    "vue-splitpane": "1.0.4",
    "vuedraggable": "2.20.0",
    "vuex": "3.1.0",
    "vxe-table": "^2.9.14",
    "xe-utils": "^2.6.3",
    "xlsx": "0.14.1",
    "vue-loader": "latest"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.10.5",
    "@babel/plugin-transform-modules-commonjs": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.10.5",
    "@babel/preset-env": "^7.10.4",
    "@cypress/code-coverage": "1.12.2",
    "@vue/cli-plugin-babel": "4.4.4",
    "@vue/cli-plugin-e2e-cypress": "^4.5.4",
    "@vue/cli-plugin-eslint": "4.4.4",
    "@vue/cli-plugin-unit-jest": "^4.4.6",
    "@vue/cli-service": "4.4.4",
    "@vue/test-utils": "^1.0.3",
    "autoprefixer": "9.5.1",
    "axios-mock-adapter": "^1.18.2",
    "babel-eslint": "10.1.0",
    "babel-jest": "^26.1.0",
    "babel-loader": "8.1.0",
    "babel-plugin-dynamic-import-node": "^2.3.3",
    "babel-plugin-istanbul": "5.2.0",
    "babel-preset-stage-3": "6.24.1",
    "chalk": "2.4.2",
    "chokidar": "2.1.5",
    "connect": "3.6.6",
    "coveralls": "3.1.0",
    "css-loader": "3.6.0",
    "cypress": "3.8.3",
    "eslint": "6.7.2",
    "eslint-plugin-vue": "6.2.2",
    "html-webpack-plugin": "3.2.0",
    "husky": "1.3.1",
    "istanbul-instrumenter-loader": "3.0.1",
    "istanbul-lib-coverage": "2.0.5",
    "jest": "^26.1.0",
    "jest-mock-axios": "^4.2.1",
    "lint-staged": "8.1.5",
    "mockjs": "1.0.1-beta3",
    "moxios": "^0.4.0",
    "node-sass": "4.14.1",
    "nyc": "14.1.1",
    "plop": "2.3.0",
    "runjs": "4.3.2",
    "sass": "1.26.2",
    "sass-loader": "8.0.2",
    "script-ext-html-webpack-plugin": "2.1.3",
    "serve-static": "1.13.2",
    "svg-sprite-loader": "4.1.3",
    "svgo": "1.2.0",
    "vue-jest": "^3.0.6",
    "vue-template-compiler": "2.6.10",
    "webpack-cli": "^3.3.12",
    "webpack": "4.44.1",
    "webpack-dev-server": "3.11.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {},
    "overrides": [
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)",
          "**/tests/unit/**/*.spec.{j,t}s?(x)"
        ],
        "env": {
          "jest": true
        }
      }
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json"
    ],
    "transform": {
      "^.+\\.(ts|html)$": "<rootDir>/node_modules/axios/index.js",
      "^.+\\.js$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(axios))"
    ],
    "globals": {
      "ts-jest": {
        "tsConfigFile": "src/tsconfig.spec.json",
        "useBabelrc": true
      },
      "__TRANSFORM_HTML__": true
    }
  },
  "nyc": {
    "check-coverage": true,
    "per-file": true,
    "include": [
      "src/components/article/*.{js,vue}",
      "src/components/table/*.{js,vue}"
    ],
    "reporter": [
      "lcov",
      "text",
      "text-summary"
    ],
    "extension": [
      ".js",
      ".vue"
    ],
    "verbose": true,
    "cache": true,
    "all": true
  }
}
